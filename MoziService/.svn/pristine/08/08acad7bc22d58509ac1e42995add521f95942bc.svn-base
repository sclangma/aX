class Loadout():
    def __init__(self, guid, name, side_name):

        self.sensors = {}
        self.guid = guid
        self.name = name
        self.side_name = side_name
        # 挂载的武器的数量
        self.strLoadWeaponCount = ""
        # 挂载的数量和挂架载荷
        self.m_LoadRatio = ""
        # 飞机的guid
        self.m_AircraftGuid = ""
        # 是否支持快速出动
        self.bQuickTurnaround = False
        # 最大飞行波次
        self.iMaxSorties = 0
        # 货物类型
        self.m_CargoType = 0

        # temp
        self.strName = ''
        self.iDBID = ''

    def delete_sub_object(self):
        """
        删除时删除子对象
        :return:
        """
        del_list = list(self.sensors.keys())
        del self.sensors
        return del_list
